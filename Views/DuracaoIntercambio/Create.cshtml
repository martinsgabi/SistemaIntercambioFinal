@model SistemaIntercambioFinal.Models.DuracaoIntercambio

@{
    ViewData["Title"] = "Create";
}



<h4>Duração do Intercambio</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="DuracaoIntercambioDescricao" class="control-label"></label>
                <input asp-for="DuracaoIntercambioDescricao" class="form-control" />
                <span asp-validation-for="DuracaoIntercambioDescricao" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary btn-cliente" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Volte pra lista</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
