@model SistemaIntercambioFinal.Models.CadastroIntercambio

@{
    ViewData["Title"] = "Create";
}



<h4>Cadastro do Intercambio</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="DestinoIntercambioId" class="control-label"></label>
                <select asp-for="DestinoIntercambioId" class ="form-control" asp-items="ViewBag.DestinoIntercambioId"></select>
            </div>
            <div class="form-group">
                <label asp-for="TipoIntercambioId" class="control-label"></label>
                <select asp-for="TipoIntercambioId" class ="form-control" asp-items="ViewBag.TipoIntercambioId"></select>
            </div>
            <div class="form-group">
                <label asp-for="DuracaoIntercambioId" class="control-label"></label>
                <select asp-for="DuracaoIntercambioId" class ="form-control" asp-items="ViewBag.DuracaoIntercambioId"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Cadastrar" class="btn btn-primary btn-cliente" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" class="voltar">Volte pra lista</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
